# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# API Keys
TUSHARE_TOKEN=your_tushare_token_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Payment Configuration
ALIPAY_APP_ID=your_alipay_app_id_here
ALIPAY_PRIVATE_KEY=your_alipay_private_key_here
ALIPAY_PUBLIC_KEY=your_alipay_public_key_here

# Feature Flags - 启用高级功能
ENABLE_PERSONAL_RESEARCH=true
ENABLE_MULTI_COMPANY_ANALYSIS=true

# AI Model Configuration
# 报告生成使用 Claude Opus 4 模型
REPORT_GENERATION_MODEL=claude-opus-4-1-20250805

# 其他功能使用 Qwen Max 模型
PERSONAL_RESEARCH_MODEL=qwen-max-latest
MULTI_COMPANY_ANALYSIS_MODEL=qwen-max-latest
CHAT_ANALYSIS_MODEL=qwen-max-latest

# AI API Configuration
OPUS4_API_KEY=your_opus4_api_key_here
QWEN_API_KEY=your_qwen_api_key_here
QWEN_API_BASE_URL=https://api.nuwaapi.com/v1

# Optional: Environment
NODE_ENV=development
